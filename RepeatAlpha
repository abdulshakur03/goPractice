package main

func RepeatAlpha(str string) string {
	result := ""
	
	for _, r := range str {
		count := 0
		if r >= 'a' && r <= 'z'{
			count = int(r - 'a' + 1)
		}else if r >= 'A' && r <= 'Z'{
			count = int(r- 'A' + 1)
		}else{
			continue
		}
		for i := 0; i < count; i++{
			result = string(r)
		}
	}
	return result

}